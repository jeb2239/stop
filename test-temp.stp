def fib_helper = (i:Int, results:Int[]):Int {
	if (results[i] != -1) {
		return results[i];
	} else {
		results[i] = fib_helper(i-1, results) + fib_helper(i-2, results);
	}
}

def fib = (i:Int, results:Int[]):Int {
	var results:Int[];
	results[0] = 0;
	results[1] = 1;

	var j:Int;
	for (j = 0; j < i; j = j + 1) {
		results[j] = -1;
	}
	return fib_helper(i, results);
}

def main = ():Int {
	printf("%d\n", fib(10));
	printf("%d\n", fib(9));
	printf("%d\n", fib(8));
	return 0;
}
